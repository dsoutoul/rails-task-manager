<h1>Edit Task</h1>
<%= form_for (@task) do |f| %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :details %>
  <br>
  <%= f.text_field :details %>
  <br>
  <br>
  <%= f.check_box :completed %>
  <%= f.label :completed %>
  <br>
  <br>
  <%= f.submit "Update task"%>
  <br>
<% end %>
<br>

